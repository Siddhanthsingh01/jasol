      "type": "string"
    },
    "Item_Weight": {
      "type": "string"
    },
    "Item_Dimensions_LxWxH": {
      "type": "string"
    },
    "Net_Quantity": {
      "type": "string"
    },
    "Included_Components": {
      "type": "string"
    },
